@model IEnumerable<TeamProjects.Models.timetable_facility>

@{
    ViewBag.Title = "Facilities";
}

<h2>Facilities</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table>
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Facility_ID)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Facility_Name)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Facility_ID)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Facility_Name)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Facility_ID }) |
            @Html.ActionLink("Details", "Details", new { id=item.Facility_ID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Facility_ID })
        </td>
    </tr>
}

</table>
