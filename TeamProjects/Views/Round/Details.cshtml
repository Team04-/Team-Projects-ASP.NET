@model TeamProjects.Models.timetable_round

@{
    ViewBag.Title = "Details for Round @Model.Round_Code";
}

<h2>Details for Round @Model.Round_Code</h2>

<table class="roundTable">
<thead>
    <tr>
        <th>
            Round Code
        </th>
        <th>
            Status
        </th>
        <th>
            Semester
        </th>
        <th>
            Start Date
        </th>
        <th>
            End Date
        </th>
    </tr>
</thead>
<tbody>
    <tr>
        <td>
            @Html.DisplayFor(modelItem => Model.Round_Code)
        </td>
        <td>
            @Html.DisplayFor(modelItem => Model.Round_Status)
        </td>
        <td>
            @Html.DisplayFor(modelItem => Model.Semester)
        </td>
        <td>
            @Html.DisplayFor(modelItem => Model.Start_Date)
        </td>
        <td>
            @Html.DisplayFor(modelItem => Model.End_Date)
        </td>
    </tr>
</tbody>
</table>
<p>
    @Html.ActionLink("Edit", "Edit", new { id = Model.Round_Code }) |
    @Html.ActionLink("Back to List", "Index")
</p>
