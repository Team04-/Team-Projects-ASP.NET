@model TeamProjects.Models.RoomBuildingViewModel

@{
    ViewBag.Title = "Details for @Model.Building_ID @Model.Room_ID";
}

<h2>Details for @Model.Building_ID @Model.Room_ID</h2>

<table class="roomTable">
<thead>
    <tr>
        <th>
            Room
        </th>
        <th>
            Building
        </th>
        <th>
            Park
        </th>
        <th>
            Capacity
        </th>
        <th>
            Type
        </th>
    </tr>
</thead>
<tbody>
    <tr>
        <td>
            @Model.Building_ID@Model.Room_ID
        </td>
        <td>
            @Html.DisplayFor(modelItem => Model.Building_Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => Model.Park_ID)
        </td>
        <td>
            @Html.DisplayFor(modelItem => Model.Capacity)
        </td>
        <td>
            @Html.DisplayFor(modelItem => Model.Type_ID)
        </td>
    </tr>
</tbody>
</table>
<p>
    @Html.ActionLink("Edit", "Edit", new { buildingID = Model.Building_ID, roomID = Model.Room_ID }) |
    @Html.ActionLink("Back to List", "Index")
</p>
