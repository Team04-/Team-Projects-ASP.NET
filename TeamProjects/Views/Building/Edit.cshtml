@model TeamProjects.Models.timetable_building

@{
    ViewBag.Title = "Edit Building "+Model.Building_ID;
}

<h2>Edit Building @Model.Building_ID</h2>

@using (Html.BeginForm()) {
    @Html.ValidationSummary(true)

    <fieldset>
        <legend>timetable_building</legend>

        <input id="Building_ID" name="Building_ID" type="hidden" value="@Model.Building_ID">
        <input id="Park_ID" name="Park_ID" type="hidden" value="@Model.Park_ID">
        <div class="editor-label">
            <label for="Building_Name">Building Name</label>
        </div>
        <div class="editor-field">
            @Html.EditorFor(model => model.Building_Name)
            @Html.ValidationMessageFor(model => model.Building_Name)
        </div>

        <p>
            <input type="submit" value="Save" />
        </p>
    </fieldset>
}

<div>
    @Html.ActionLink("Back to List", "Index")
</div>

@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}
