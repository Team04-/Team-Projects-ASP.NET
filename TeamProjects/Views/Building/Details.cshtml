@model TeamProjects.Models.timetable_building

@{
    ViewBag.Title = "Details for @Model.Building_Name";
}

<h2>Details for @Model.Building_Name</h2>

<table class="buildingTable">
<thead>
    <tr>
        <th>
            Building Code
        </th>
        <th>
            Name
        </th>
    </tr>
</thead>
<tbody>
    <tr>
        <td>
            @Html.DisplayFor(modelItem => Model.Building_ID)
        </td>
        <td>
            @Html.DisplayFor(modelItem => Model.Building_Name)
        </td>
    </tr>
</tbody>
</table>

<p>
    @Html.ActionLink("Edit", "Edit", new { id=Model.Building_ID }) |
    @Html.ActionLink("Back to List", "Index")
</p>
