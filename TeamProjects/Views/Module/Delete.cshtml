@model TeamProjects.Models.timetable_module

@{
    ViewBag.Title = "Delete";
}

<h2>Delete</h2>

<h3>Are you sure you want to delete this?</h3>
<table class="moduleTable">
<thead>
    <tr>
        <th>
            Module Code
        </th>
        <th>
            Part Code
        </th>
        <th>
            Module Title
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Active)
        </th>
    </tr>
</thead>
<tbody>
    <tr>
        <td>
            @Model.Department_Code@Model.Module_Code
        </td>
        <td>
            @Html.DisplayFor(modelItem => Model.Part_Code)
        </td>
        <td>
            @Model.Module_Title
        </td>
        <td>
            @Html.DisplayFor(modelItem => Model.Active)
        </td>
    </tr>
</tbody>
</table>
@using (Html.BeginForm()) {
    <p>
        <input type="submit" value="Delete" /> |
        @Html.ActionLink("Back to List", "Index")
    </p>
}
