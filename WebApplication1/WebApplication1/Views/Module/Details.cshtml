@using Microsoft.AspNet.Identity
@model WebApplication1.Models.timetable_module

@{
    ViewBag.Title = Model.Department_Code;
    ViewBag.Title += Model.Module_Code;
}

<h2>@(Model.Department_Code)@(Model.Module_Code) Details</h2>

<div>
	<hr />
    <dl class="dl-horizontal">
        <dt>
            Module Code
        </dt>

        <dd>
            @(Model.Department_Code)@(Model.Module_Code)
        </dd>

        <dt>
            Module Title
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Module_Title)
        </dd>

        <dt>
            Active
        </dt>

        <dd>
            @Html.Raw(Model.Active)
        </dd>

    </dl>
</div>
<p>
    @Html.ActionLink("Edit", "Edit", new { /* id = Model.PrimaryKey */ }) |
    @Html.ActionLink("Back to List", "Index")
</p>
